                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        Producto.CodigoAvatar, Producto.Nombre, RegistroBodega.Nombre AS Bodega, RegistroIngresoProducto.IDProducto, SUM(RegistroIngresoProducto.CantidadXEmpaque) AS Unidades, 
                         SUM(RegistroIngresoProducto.CantidadXEmpaque * RegistroIngresoProducto.CostoXEmpaque) AS Costo, RegistroProducto.FechaIngreso AS Fecha
FROM            RegistroIngresoProducto INNER JOIN
                         RegistroProducto ON RegistroIngresoProducto.IDIngresoProducto = RegistroProducto.IDIngresoProducto INNER JOIN
                         Producto ON RegistroIngresoProducto.IDProducto = Producto.IDProducto INNER JOIN
                         RegistroBodega ON RegistroProducto.IDRegistroBodega = RegistroBodega.IDRegistroBodega
WHERE        (RegistroIngresoProducto.Estado = 1) and RegistroProducto.FechaIngreso between @FechaInicio and @FechaFin
GROUP BY Producto.CodigoAvatar, Producto.Nombre, RegistroIngresoProducto.IDProducto, RegistroIngresoProducto.IDProducto, RegistroProducto.FechaIngreso, RegistroBodega.Nombre</CommandText>
